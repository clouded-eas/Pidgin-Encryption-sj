#: config_ui.c:89
msgid "Bad key size"
msgstr "Ungültige Schlüsselgröße"

#: config_ui.c:95
msgid "Keys < 512 bits are VERY insecure"
msgstr "Schlüssel kürzer als 512 Bit sind SEHR unsicher."

#: config_ui.c:101
msgid ""
"Keys > 4096 bits will cause extreme\n"
"message bloat, causing problems with\n"
"message transmission"
msgstr ""
"Schlüssel > 4096 Bit vergrößern\n"
"Nachrichten unnötig und verursachen\n"
"Probleme bei der Übertragung der Nachrichten"

